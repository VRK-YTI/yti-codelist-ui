    "buildProduction": "node -- node_modules/@angular/cli/bin/ng build --prod",
    "create-translations": "node -- node_modules/@biesbjerg/ngx-translate-extract/bin/cli.js --input ./src/app --output ./po/template.pot --format=pot --clean"
